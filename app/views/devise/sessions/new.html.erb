<div class="panel panel-default center-block">
<div class="panel-heading">Step Up Women's Nework Sign In</div>
<div class="panel-body">

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
	<% if resource.errors.any? %>
		<div class="alert alert-info">
			<h2><%= pluralize(resource.errors.count, "error") %> prohibited this page from being saved:</h2>
		    <ul>
		      <% resource.errors.full_messages.each do |msg| %>
		    		<li><%= msg %></li>
		      <% end %>
		    </ul>
		</div>
	<% end %>

  <div>
  <%= f.email_field :email, placeholder:"Email" %></div>
  <br />
  <div>
  <%= f.password_field :password,placeholder:"Password" %></div>
  <br />

  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
  <% end -%>

  <div><%= f.submit "Sign in", class: "btn btn-primary btn-sm" %></div>
<% end %>

<%= render "devise/shared/links" %>
